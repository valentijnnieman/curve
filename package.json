{
  "name": "curve-visual",
  "version": "0.1.0",
  "scripts": {
    "db-migrate": "sequelize db:migrate",
    "start": "NODE_ENV=production nodemon server",
    "dev": "NODE_ENV=development nodemon server",
    "test": "npm run test.backend",
    "test.backend": "NODE_ENV=test mocha --exit",
    "test.backend.watch": "NODE_ENV=test mocha --watch",
    "test.frontend": "cd react-ui/ && npm run test",
    "heroku-postbuild": "cd react-ui/ && npm install && npm install --only=dev --no-shrinkwrap && npm run build"
  },
  "cacheDirectories": [
    "node_modules",
    "react-ui/node_modules"
  ],
  "dependencies": {
    "@types/jest": "^23.3.9",
    "bcrypt": "^3.0.7",
    "cookie-parser": "^1.4.4",
    "cookie-session": "^1.3.3",
    "express": "^4.18.2",
    "express-validator": "^5.3.1",
    "morgan": "^1.9.1",
    "nodemon": "^1.19.4",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "pg": "^7.4.3",
    "pg-hstore": "^2.3.2",
    "sequelize": "^5.21.2",
    "sequelize-auto-migrations": "^1.0.3",
    "sequelize-cli": "^5.5.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/valentijnnieman/curve.git"
  },
  "keywords": [
    "web audio",
